[
    {
      "volumesFrom": [],
      "memory": ${memory},
      "portMappings": [
        { 
          "hostPort": 0,
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "mountPoints": [
        {
          "containerPath": "/opt/nodeapp/config",
          "sourceVolume": "docker_app_config",
          "readOnly": null
        }
      ],
      "name": "docker_app",
      "image": "${docker_container_image}",
      "entryPoint" : [ "./run-aws-ecs.sh" ],
      "command" : [ "start" ],
      "cpu": ${cpu}
    }
]

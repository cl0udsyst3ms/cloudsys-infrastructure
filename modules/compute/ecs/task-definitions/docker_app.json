[
    {
      "volumesFrom": [],
      "memory": ${memory},
      "portMappings": [
        { 
          "hostPort": 0,
          "containerPort": 9999,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "mountPoints": [
        {
          "containerPath": "/opt/nodeapp/config",
          "sourceVolume": "/etc/liga/config",
          "readOnly": null
        },
        {
          "containerPath": "/opt/nodeapp/logs",
          "sourceVolume": "/etc/liga/logs"
        }
      ],
      "name": "docker_app",
      "image": "${docker_container_image}",
      "entryPoint" : [ "/bin/loop.sh" ],
      "command" : [ "start" ],
      "cpu": ${cpu}
    }
]
